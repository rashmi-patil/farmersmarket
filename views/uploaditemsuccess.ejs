		<legend>Enter item details:</legend>
		<div class="control-group">
	        <label class="control-label">Item Name</label>
			<div class="controls">
			    <div class="input-prepend">
				<span class="add-on"><i class="icon-envelope"></i></span>
					<input type="text" class="input-xlarge" id="fname" name="itemname" placeholder="Item Name">
				</div>
			</div> 
		</div>
		<div class="control-group ">
	        <label class="control-label">In Stock</label>
			<div class="controls">
			    <div class="input-prepend">
				<span class="add-on"><i class="icon-user"></i></span>
					<input type="text" class="input-xlarge" id="lname" name="instock" placeholder="In Stock">
				</div>
			</div>
		</div>
		<div class="control-group">
	        <label class="control-label">Unit price</label>
			<div class="controls">
			    <div class="input-prepend">
